{
  "name": "storybook-addon-linguijs",
  "version": "1.0.10",
  "description": "Addon to provide locale switcher and linguijs react for storybook",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm-run-all --sequential clean tsc",
    "clean": "del-cli ./dist",
    "prepare": "npm run build",
    "storybook": "start-storybook -p 9001",
    "tsc": "tsc --build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HichemBenChaaben/storybook-addon-linguijs.git"
  },
  "keywords": [
    "storybook",
    "storybook-addon",
    "react-lingui",
    "i18n",
    "l10n"
  ],
  "license": "MIT",
  "author": "Hichem benchaaben",
  "dependencies": {
    "@lingui/core": "^2.8.3",
    "@lingui/react": "^2.8.3",
    "@types/lingui__core": "2.7.0",
    "@types/lingui__react": "2.8.1",
    "@types/node": "13.7.6",
    "@types/react": "16.9.23",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@lingui/macro": "^2.8.3",
    "@storybook/addons": "^5.3.14",
    "@storybook/channels": "^5.3.14",
    "@storybook/react": "^5.3.14",
    "del-cli": "3.0.0",
    "npm-run-all": "4.1.5",
    "storybook": "^1.0.0",
    "typescript": "3.8.2"
  },
  "peerDependencies": {
    "@lingui/react": ">=2.8.0",
    "@storybook/addons": ">= 3.2.16",
    "@storybook/react": ">= 3.2.16",
    "react": ">= 15.4.0"
  }
}
